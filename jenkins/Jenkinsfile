    agent {
        docker {
            image 'maven:3.9.0'
            args '-v /root/.m2:/root/.m2'
        }
    }
    stages {
        stage('Deploy') {
            steps {
                script {
                    sh 'sudo apt-get update && sudo apt-get install -y sshpass'
                    sh 'sshpass -p "Es=NDW3yXLjzT(H" ssh -o StrictHostKeyChecking=no root@8.219.41.186 ls'
                }
            }
        }
    }
}
